{
  "name": "ImprovedGistPager",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Improve Gist pager.",
  "author": "universato",
  "homepage_url": "https://github.com/universato/",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [{
    "matches": ["https://gist.github.com/*", "http://gist.github.com/*"],
    "run_at": "document_end",
    "js": ["script.js"],
    "all_frames": true
  }]
}
